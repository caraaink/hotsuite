{
  "version": 2,
  "builds": [
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/refresh-token.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "api/upload.js"
    },
    {
      "src": "/api/refresh-token",
      "dest": "api/refresh-token.js"
    },
    {
      "src": "/style.css",
      "dest": "public/style.css"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
